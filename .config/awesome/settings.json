{
    "program": {
        "autostart": [
            {
                "program": "autorandr --change",
                "type": "startup"
            }
        ]
    },
    "theme": {
        "theme": "catppuccin",
        "variant": "mocha",
        "icon_theme": "Papirus-Dark",
        "xresources_enabled": true,
        "gtk": {
            "enabled": true
        },
        "nvim": {
            "enabled": true
        },
        "alacritty": {
            "enabled": true
        },
        "discord": {
            "enabled": true,
            "install": "flatpak",
            "client_mod": "Vencord"
        },
        "firefox": {
            "enabled": true,
            "profile": "rt5lr27b.default-release",
            "install": "native"
        }
    },
    "locale": {
        "langs": [
            "us"
        ]
    }
}
